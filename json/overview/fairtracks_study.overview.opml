<body>
<outline const="FAIRification of Genomic Tracks JSON Schema - Study" text="#title" type="string" />
<outline const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of '$id' in the linked schema" format="iri" required="false" text="@schema" type="string" />
<outline description="Global study identifier, resolvable by identifiers.org" examples="GEO:GSE35583" format="curie" matchType="canonical" namespace="geo|ega.study" required="true" text="global_id" type="string" />
<outline description="Submitter-local identifier (within investigation/hub) for study" examples="UW_ChipSeq" required="true" text="local_id" type="string" unique="true" />
<outline description="Name of the study" examples="Histone Modifications by ChIP-seq from ENCODE/University of Washington" required="true" text="study_name" type="string" />
<outline required="false" text="publications" type="array" minItems="1">
<outline text="" description="Pubmed identifiers (dataset or publication)" examples="22955617" format="curie" matchType="loose" namespace="pubmed" type="string" />
</outline>
<outline description="Contact information for study" required="true" text="contact" type="object">
<outline description="Name of contact person/organization" examples="ENCODE DCC" required="true" text="name" type="string" />
<outline anyOf="true" description="E-mail to contact person/organization" examples="encode-help@lists.stanford.edu" format="idn-email" required="false" text="e-mail" type="string" />
<outline anyOf="true" comments="ORCID id - Needs GDPR consent" description="ORCID id to contact person" examples="0000-0000-0000-0000" format="curie" matchType="loose" namespace="orcid" required="false" text="orcid_id" type="string" />
</outline>
<outline description="Main phenotype (e.g. disease) which is studied" required="true" text="phenotype" type="object">
<outline description="IRI linking to an EFO ontology term" examples="http://purl.obolibrary.org/obo/PATO_0000461" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" text="term_iri" type="string" />
<outline autogenerated="true" description="Exact value according to the EFO ontology used" examples="Normal" required="false" text="term_value" type="string" />
</outline>
</body>
