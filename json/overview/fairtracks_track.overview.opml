<outline const="FAIRification of Genomic Tracks JSON Schema - Track" text="#title" type="string" />
<outline const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_track.schema.json" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of '$id' in the linked schema" format="iri" required="false" text="@schema" type="string" />
<outline autogenerated="true" description="Global track identifier, resolvable by identifiers.org [to be created by us]" examples="fairtracks:1|fairtracks:2|fairtracks:3|fairtracks:4" format="curie" matchType="basic" namespace="fairtracks" required="true" text="global_id" type="string" />
<outline description="Submitter-local identifier (within investigation/hub) for study (in curie-format, if applicable)" examples="encode:ENCFF625ZYB|encode:ENCFF718EPO|encode:ENCFF717PIO|encode:ENCFF955LOC" required="true" text="local_id" type="string" unique="true" />
<outline description="Reference to the experiment of the track (using the submitter-local identifier of the sample)" examples="encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP" required="true" text="experiment_ref" type="string" />
<outline description="A IRI to the track data file" examples="https://www.encodeproject.org/files/ENCFF625ZYB/@@download/ENCFF625ZYB.bed.gz|https://www.encodeproject.org/files/ENCFF718EPO/@@download/ENCFF718EPO.bigBed|https://www.encodeproject.org/files/ENCFF717PIO/@@download/ENCFF717PIO.bigWig|https://www.encodeproject.org/files/ENCFF955LOC/@@download/ENCFF955LOC.bigWig" format="iri" required="true" text="file_iri" type="string" />
<outline autogenerated="true" description="Name of the track file" examples="r1.narrowPeak.gz|r1.narrowPeak.bb|r1.fc_signal.bw|r1.pvalue_signal.bw" required="false" text="file_name" type="string" />
<outline comments=" (min of characters limits [14?])" description="A short label of the track file [less than 14 characters?]" examples="ENCFF625ZYB|ENCFF718EPO|ENCFF717PIO| ENCFF955LOC" required="true" text="label_short" type="string" />
<outline description="A long label of the track file" examples="H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF625ZYB|H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF718EPO|H3K4me3 ChIP-seq of B cell fold change over control rep1 ENCSR000DQP - ENCFF717PIO|H3K4me3 ChIP-seq of B cell signal p-value rep1 ENCSR000DQP - ENCFF955LOC" required="true" text="label_long" type="string" />
<outline description="An assembly identifier (using UCSC nomenclature)" examples="GRCh38|GRCh38|GRCh38|GRCh38" required="true" text="genome_assembly" type="string" />
<outline description="File format of the track data file" required="true" text="file_format" type="object">
<outline description="File format (IRI linking to an EDAM ontology term)" examples="http://edamontology.org/format_3613|http://edamontology.org/format_3004|http://edamontology.org/format_3006|http://edamontology.org/format_3006" format="term" matchType="exact" ontology="http://edamontology.org/EDAM_1.21.owl" required="true" text="term_iri" type="string" />
<outline autogenerated="true" description="File format (exact value according to the EDAM ontology used)" examples="ENCODE narrow peak format|bigBed|bigWig|bigWig" required="false" text="term_value" type="string" />
</outline>
<outline description="Type of content represented by the track data file (e.g. narrow peaks, signal, region, â€¦)" required="true" text="content_type" type="object">
